<!-- main -->
<div class="container mt-5">
    <h2>Order History</h2>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Image</th>
                <th scope="col">Product</th>
                <th scope="col">Quantity</th>
                <th scope="col">Price</th>
                <th scope="col">Date</th>
                <th scope="col">Details</th>
                <!-- <th scope="col">Download Invoice</th> -->
            </tr>
        </thead>
        <tbody>
            <% dataOrder.forEach(function(item) { %>
                <tr>
                    <th scope="row"><img src="<%= item.img %>" alt="Product Image" style="height: 80px;"></th>
                    <td><%= item.product %><br>Order id : <span><%= item.orderId %></span></td>
                    <td><%= item.quentity %></td>
                    <% if (item.amountPaid) { %>
                        <td>₹ <%= item.amountPaid %></td>
                    <% } else { %>
                        <td>₹ <%= item.price %></td>
                    <% } %>
                    <td><%= item.orderDate.toLocaleDateString() %></td>
                    <td><a href="/historyOrder?orderId=<%= item.orderId %>&product=<%= item.product %>" class="btn btn-dark">Details</a></td>
                    <td><a href="/invoice?orderId=<%= item.orderId %>&product=<%= item.product %>" class="btn btn-primary">Download</a></td>
                </tr>
            <% }); %>
            
            <!-- Add more rows as needed -->
        </tbody>
    </table>
</div>
